#, docstring
msgid ""
"<expression>\n"
"\n"
"        Uses Google's calculator to calculate the value of <expression>.\n"
"        "
msgstr ""

#: plugin.py:341
#, docstring
msgid ""
"<phone number>\n"
"\n"
"        Looks <phone number> up on Google.\n"
"        "
msgstr ""

