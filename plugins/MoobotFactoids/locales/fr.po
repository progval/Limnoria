msgid "%s is %s"
msgstr "%s est %s"

#: plugin.py:377
msgid "Factoid %q is locked."
msgstr "La factoid %q est verrouillée"

#: plugin.py:384
msgid "Factoid %q not found."
msgstr "Factoid %q non trouvée."

